    "@aws-sdk/credential-providers": "^3.873.0",
    "@heroicons/react": "^2.2.0",
    "clsx": "^1.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.263.1",
    "pdfjs-dist": "^5.4.54",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "react-syntax-highlighter": "^15.6.3",
    "recharts": "^3.1.2",
    "rehype-katex": "^7.0.1",
    "remark-breaks": "^4.0.0",
