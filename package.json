    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.27",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.3",
