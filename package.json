{
  "name": "claude-interface-clone",
  "version": "1.0.0",
  "type": "module",
  "description": "Claude AI 웹 인터페이스 클론",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "serve -s dist",
    "dev:chat": "vite --config vite.config.chat.js --port 3003",
    "build:chat": "vite build --config vite.config.chat.js",
    "serve:chat": "serve -s dist-chat -p 3004",
    "deploy:chat": "chmod +x scripts/deploy-chat-only.sh && ./scripts/deploy-chat-only.sh",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\""
  },
  "dependencies": {
    "@aws-amplify/auth": "^6.15.0",
    "@aws-amplify/core": "^6.13.1",
